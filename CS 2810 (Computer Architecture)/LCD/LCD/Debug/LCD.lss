000066 e300                          ldi     temp, lcd_FunctionReset         ; first part of reset sequence
000067 940e 00c3                     call    lcd_write_4
000069 e00a                          ldi     temp, 10                        ; 4.1 mS delay (min)
00006a 940e 00db                     call    delayTx1mS
                                 
00006c e300                          ldi     temp, lcd_FunctionReset         ; second part of reset sequence
00006d 940e 00c3                     call    lcd_write_4
00006f ec08                          ldi     temp, 200                       ; 100 uS delay (min)
000070 940e 00e9                     call    delayTx1uS
                                 
000072 e300                          ldi     temp, lcd_FunctionReset         ; third part of reset sequence
000073 940e 00c3                     call    lcd_write_4
000075 ec08                          ldi     temp, 200                       ; this delay is omitted in the data sheet
000076 940e 00e9                     call    delayTx1uS
                                 
