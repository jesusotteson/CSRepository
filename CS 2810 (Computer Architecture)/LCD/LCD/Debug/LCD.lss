000078 e208                          ldi     temp, lcd_FunctionSet4bit       ; set 4-bit mode
000079 940e 00c3                     call    lcd_write_4
00007b e500                          ldi     temp, 80                        ; 40 uS delay (min)
00007c 940e 00e9                     call    delayTx1uS
                                 
